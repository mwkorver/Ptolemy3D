<jnlp spec="1.0+" codebase="./" href="jogl.jnlp">
	<information>
		<title>Java Binding to the OpenGL API</title>
		<vendor>Sun Microsystems, Inc.</vendor>
		<homepage href="http://jogl.dev.java.net/"/>
   		<description>Java Binding to the OpenGL API - JSR-231</description>
    		<description kind="short">Java programming language binding for the OpenGL 3D graphics API.</description>
		<offline-allowed/>
	</information>

	<security>
		<all-permissions/>
	</security>

	<resources>
		<j2se version="1.4+"/>
		<jar href="../lib//jogl.jar"/>
		<property name="sun.java2d.noddraw" value="true"/>
		<extension name="gluegen" href="gluegen.jnlp" />
	</resources>
	<resources os="Windows" arch="x86">
		<nativelib href="../lib//jogl-natives-windows-i586.jar"/>
	</resources>
	<resources os="Windows" arch="amd64">
		<nativelib href="../lib//jogl-natives-windows-amd64.jar"/>
	</resources>
	<resources os="Windows" arch="x86_64">
		<nativelib href="../lib//jogl-natives-windows-amd64.jar"/>
	</resources>
	<resources os="Linux" arch="i386">
		<nativelib href="../lib//jogl-natives-linux-i586.jar"/>
	</resources>
	<resources os="Linux" arch="x86">
		<nativelib href="../lib//jogl-natives-linux-i586.jar"/>
	</resources>
	<resources os="Linux" arch="amd64">
		<nativelib href="../lib//jogl-natives-linux-amd64.jar"/>
	</resources>
	<resources os="Linux" arch="x86_64">
		<nativelib href="../lib//jogl-natives-linux-amd64.jar"/>
	</resources>
	<resources os="Mac OS X" arch="ppc">
		<nativelib href="../lib//jogl-natives-macosx-ppc.jar"/>
	</resources>
	<resources os="Mac OS X" arch="i386">
		<nativelib href="../lib//jogl-natives-macosx-universal.jar"/>
	</resources>
	<resources os="Mac OS X" arch="x86_64">
		<nativelib href="../lib//jogl-natives-macosx-universal.jar"/>
	</resources>
	<resources os="SunOS" arch="sparc">
		<nativelib href="../lib//jogl-natives-solaris-sparc.jar" />
	</resources>
	<resources os="SunOS" arch="sparcv9">
		<nativelib href="../lib//jogl-natives-solaris-sparcv9.jar" />
	</resources>
	<resources os="SunOS" arch="x86">
		<nativelib href="../lib//jogl-natives-solaris-i586.jar" />
	</resources>
	<resources os="SunOS" arch="amd64">
		<nativelib href="../lib//jogl-natives-solaris-amd64.jar" />
	</resources>
	<resources os="SunOS" arch="x86_64">
		<nativelib href="../lib//jogl-natives-solaris-amd64.jar" />
	</resources>
	<component-desc />
</jnlp>